#include <iostream>
using namespace std;
int main() {
    int s;
    cout<<"Enter the size of the array to be created: ";
    cin>>s;
    int a1[s];
    cout<<"Enter "<<s<<" elements of array: ";
    for(int j=0;j<s;j++){
    cin>>a1[j];
    }
    cout<<endl;
    cout<<"The numbers are: "<<endl;
    for(int i=0;i<s;i++){
    cout <<a1[i]<<endl;
    }
    return 0;
}
/* Output
Enter the size of the array to be created: 3
Enter 3 elements of array: 23
12
45

The numbers are: 
23
12
45
*/
